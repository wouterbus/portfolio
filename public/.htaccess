<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /
  # Skip existing files/directories
  RewriteCond %{REQUEST_FILENAME} -f [OR]
  RewriteCond %{REQUEST_FILENAME} -d
  RewriteRule ^ - [L]
  # Serve prerendered OG pages to common crawler user agents
  RewriteCond %{HTTP_USER_AGENT} (Twitterbot|Slackbot|facebookexternalhit|LinkedInBot|Discordbot|WhatsApp) [NC]
  RewriteRule ^project/(.*)$ /prerender/project/$1/index.html [L]
  # Redirect all other requests to index.html
  RewriteRule ^ index.html [L]
</IfModule>


